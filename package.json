{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@testing-library/dom": "^10.4.0",
    "antd": "^5.24.9",
    "axios": "^1.8.4",
    "chart.js": "^4.4.9",
    "html-react-parser": "^5.2.3",
    "millify": "^6.1.0",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint . --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.*' --ignore-pattern '**/*.spec.*' --ignore-pattern '.eslintrc.js' --ignore-pattern '.lintstagedrc.js' --ignore-pattern 'babel.config.js' --ignore-pattern 'jest.config.js' --ignore-pattern 'jest.setup.js' --ignore-pattern 'setupTests.js' --ignore-pattern 'src/mocks/**' --ignore-pattern 'src/setupTests.js'",
    "lint:fix": "eslint . --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.*' --ignore-pattern '**/*.spec.*' --ignore-pattern '.eslintrc.js' --ignore-pattern '.lintstagedrc.js' --ignore-pattern 'babel.config.js' --ignore-pattern 'jest.config.js' --ignore-pattern 'jest.setup.js' --ignore-pattern 'setupTests.js' --ignore-pattern 'src/mocks/**' --ignore-pattern 'src/setupTests.js' --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.1",
    "@reduxjs/toolkit": "^2.7.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "msw": "^1.3.5",
    "prettier": "^3.5.3"
  }
}
